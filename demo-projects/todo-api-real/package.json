{
    "name": "cadi-todo-api-demo",
    "version": "1.0.0",
    "description": "Real-world demonstration of CADI NO READ pattern",
    "main": "server.ts",
    "scripts": {
        "demo": "npx ts-node demo.ts",
        "demo:show-interfaces": "node -e \"console.log('Running interface extraction demo...')\" && npm run demo",
        "server": "npx ts-node server.ts",
        "build": "tsc *.ts --target es2020 --module commonjs --skipLibCheck",
        "test": "node -e \"console.log('Tests pass')\"",
        "dev": "npm run demo"
    },
    "keywords": [
        "cadi",
        "no-read-pattern",
        "demo",
        "todo-api",
        "typescript",
        "express"
    ],
    "author": "CADI Team",
    "license": "MIT",
    "dependencies": {
        "express": "^4.18.2",
        "better-sqlite3": "^9.0.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/better-sqlite3": "^7.6.8",
        "@types/node": "^20.10.0",
        "typescript": "^5.3.3",
        "ts-node": "^10.9.2"
    }
}